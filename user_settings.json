{
  // Editor
  "editor.formatOnSave": true,
  "editor.fontWeight": 500,
  "editor.lineHeight": 2.15,
  "editor.fontFamily": "'JetbrainsMono Nerd Font'",
  "editor.fontSize": 13.5,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.tokenColorCustomizations": {
    "functions": "#A9DC76",
    "keywords": "#FF6188",
    "[Monokai Pro]": {
      "textMateRules": [
        {
          "scope": ["variable.parameter.ts", "variable.parameter.js"],
          "settings": {
            "foreground": "#F59762",
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "keyword.operator.logical",
            "keyword.operator.type",
            "keyword.operator.assignment.ts"
          ],
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "source.css variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground": "#78DCE8"
          }
        },
        {
          "scope": [
            "meta.function.parameter variable",
            "meta.function.parameter punctuation.definition.variable.php"
          ],
          "settings": {
            "foreground": "#4ca03c"
          }
        },
        {
          "scope": [
            "variable.language.this.js",
            "constant.language",
            "punctuation.definition.keyword.scss",
            "entity.name.tag.html",
            "entity.name.tag.reference.scss",
            "source.css variable.scss",
            "entity.other.attribute-name.parent-selector-suffix.css punctuation.definition.entity.css"
          ],
          "settings": {
            "foreground": "#FF6188"
          }
        },
        {
          "scope": [
            "variable.language.this.php",
            "variable.other.constant",
            "punctuation.definition.variable.php",
            "entity.other.inherited-class"
          ],
          "settings": {
            "foreground": "#FCFCFA",
            "fontStyle": ""
          }
        },
        {
          "scope": [
            "text.html.php meta.embedded.block.php source.php meta.class.php entity.name.type.class.php",
            "text.html.php meta.embedded.block.php source.php meta.class.php meta.class.body.php support.class.php"
          ],
          "settings": {
            "foreground": "#A9DC76"
          }
        },
        {
          "scope": [
            "punctuation.definition.string.template.begin",
            "punctuation.definition.string.template.end",
            "meta.attribute.id.html string",
            "meta.attribute.class.html string"
          ],
          "settings": {
            "foreground": "#FFD866"
          }
        },
        {
          "scope": "punctuation.section.embedded.begin.php",
          "settings": {
            "fontStyle": "italic"
          }
        },
        // VueJS
        {
          "scope": [
            "source.vue text.html.derivative meta.tag.other.unrecognized.html.derivative entity.name.tag.html",
            "source.vue entity.name.tag.template.html.vue",
            "source.vue entity.name.tag.script.html.vue",
            "source.vue entity.name.tag.style.html.vue"
          ],
          "settings": {
            "foreground": "#24BDA4"
          }
        },
        {
          "scope": [
            "source.vue source.ts meta.var.expr.ts meta.function-call.ts entity.name.function.ts",
            "punctuation.attribute-shorthand.event.html.vue",
            "source.vue text.html.derivative keyword.control",
            "text.html.php meta.embedded.block.php source.php meta.class.php meta.class.body.php meta.method-call.static.php entity.name.function.php"
          ],
          "settings": {
            "foreground": "#78DCE8"
          }
        },
        {
          "scope": [
            "source.vue source.ts meta.type.parameters.ts meta.object.type.ts meta.field.declaration.ts meta.definition.property.ts variable.object.property.ts"
          ],
          "settings": {
            "foreground": "#FCFCFA"
          }
        },
        {
          "scope": "source.vue source.ts meta.type.parameters.ts meta.object.type.ts meta.field.declaration.ts meta.type.annotation.ts meta.object.type.ts meta.field.declaration.ts meta.type.annotation.ts support.type.primitive.ts",
          "settings": {
            "foreground": "#FF6188"
          }
        },
        {
          "scope": [
            "source.vue text.html.derivative punctuation.separator",
            "source.vue text.html.derivative punctuation.definition.string"
          ],
          "settings": {
            "foreground": "#FFD866"
          }
        },
        // Blade
        {
          "scope": [
            "text.html.php.blade punctuation.separator.key-value.html",
            "text.html.php.blade punctuation.definition.string.begin.html",
            "text.html.php.blade punctuation.definition.string.end.html",
            "text.html.php punctuation.separator.key-value.html",
            "text.html.php punctuation.definition.string.begin.html",
            "text.html.php punctuation.definition.string.end.html"
          ],
          "settings": {
            "foreground": "#FFD866"
          }
        },
        {
          "scope": [
            "text.html.php meta.embedded.block.php source.php meta.use.php support.class.php",
            "entity.name.type.namespace.php",
            "text.html.php meta.embedded.block.php source.php meta.class.php meta.class.body.php meta.function.php support.class.php"
          ],
          "settings": {
            "foreground": "#FCFCFA"
          }
        },
        // Parameters
        {
          "scope": [
            "text.html.php meta.embedded.block.php source.php meta.class.php meta.class.body.php meta.function.php meta.function.parameters.php meta.function.parameter.typehinted.php variable.other.php",
            "text.html.php meta.embedded.block.php source.php meta.class.php meta.class.body.php meta.function.php meta.function.parameters.php meta.function.parameter.typehinted.php variable.other.php punctuation.definition.variable.php",
            // "source.php meta.class.body.php variable.other.php"
            "meta.function.parameter variable",
            "meta.function.parameter punctuation.definition.variable.php"
          ],
          "settings": {
            "foreground": "#F59762",
            "fontStyle": "italic"
          }
        },
        {
          "scope": "strong",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "constant.language",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "storage.type",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "storage.modifier",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "keyword",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "variable.language",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground": "#413f39"
          }
        }
      ]
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "parameter": {
        "foreground": "#F59762",
        "fontStyle": "italic"
      }
    }
  },
  "editor.guides.indentation": true,
  // Workbench
  "workbench.colorTheme": "Monokai Pro",
  "workbench.tree.indent": 18,
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#87CEFA",
    "editorBracketHighlight.foreground2": "#FFD700",
    "editorBracketHighlight.foreground3": "#87CEFA",
    "editorBracketHighlight.foreground4": "#DA70D6",
    "editorBracketHighlight.foreground5": "#97c26c",
    "editorBracketHighlight.foreground6": "#abb2c0",
    "editorBracketHighlight.unexpectedBracket.foreground": "#db6165",
    "phpParameterHint.hintForeground": "#FCFCFA",
    "phpParameterHint.hintBackground": "#262427",
    // Sidebar
    "sideBar.background": "#2E2A2E",
    "sideBar.border": "#353337",
    // ActivityBar
    "activityBar.background": "#2E2A2E",
    "activityBar.border": "#353337",
    // StatusBar
    "statusBar.background": "#2E2A2E",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.border": "#353337",
    // Terminal
    "terminal.background": "#2E2A2E",
    "terminal.integrated.fontFamily": "'JetbrainsMono Nerd Font', 'monospace'",
    // Git colors
    "gitDecoration.ignoredResourceForeground": "#5B595C",
    "gitDecoration.modifiedResourceForeground": "#80CBC4",
    "gitDecoration.addedResourceForeground": "#C3E887",
    "gitDecoration.untrackedResourceForeground": "#F77669"
  },
  "workbench.tree.renderIndentGuides": "none",
  "workbench.iconTheme": "a-file-icon-vscode",
  // Explorer
  "explorer.decorations.badges": false,
  "explorer.compactFolders": false,
  "workbench.editor.showTabs": "none",
  "explorer.autoReveal": "focusNoScroll",
  // Terminal
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.6,
  "terminal.integrated.cursorBlinking": true,
  // Apc
  "apc.imports": [
    "/Users/epic/Documents/vscode-custom/custom-css.css",
    "/Users/epic/Documents/vscode-custom/custom-js.js"
  ],
  "apc.listRow": {
    "lists": [
      "customview-tree",
      "results",
      "open-editors",
      "explorer-folders-view",
      "outline-tree",
      "scm-view",
      "debug-view-content",
      "debug-breakpoints",
      "tree"
    ],
    "height": 32,
    "fontSize": 13.5
  },
  // "apc.electron": {
  //   "titleBarStyle": "hiddenInset",
  //   "trafficLightPosition": {
  //     "x": 7,
  //     "y": 5
  //   }
  // },
  "apc.statusBar": {
    "height": 30
  },
  // Disable auto-update
  "update.mode": "none",
  // Auto-generate
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.embeddedhtml": "html",
    "*.module": "php",
    ".php_cs.dist": "php",
    ".php_cs": "php",
    ".pre": "php",
    "artisan": "php"
  },
  "namespaceResolver.showMessageOnStatusBar": true,
  "workbench.startupEditor": "none",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
    "*.dart": "${capture}.g.dart",
    "composer.json": "composer.lock",
    "psalm.xml": "psalm-baseline.xml",
    ".php-cs-fixer.php": ".php-cs-fixer.cache",
    "phpunit.xml": ".phpunit.result.cache",
    "*.php": "${basename}Test.php"
  },
  "emmet.includeLanguages": {
    "blade.php": "html",
    "twig": "html"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "_ide_helper.php": true,
    "_ide_helper_models.php": true,
    ".php_cs.cache": true,
    ".phpstorm.meta.php": true,
    ".phpunit.result.cache": true,
    "out": true,
    "dist": true
  },
  "workbench.editor.titleScrollbarSizing": "large",
  "workbench.tree.expandMode": "doubleClick",
  "workbench.list.openMode": "doubleClick",
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#87CEFA",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "workbench.fontAliasing": "antialiased",
  "workbench.tree.enableStickyScroll": false,
  "workbench.list.horizontalScrolling": true,
  "breadcrumbs.enabled": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "update.showReleaseNotes": false,
  "git.openRepositoryInParentFolders": "never",
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "cSpell.userWords": ["ARTBOARD", "Avenir", "gsap", "htmltext"],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSave": "afterDelay",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "laravel-pint.enable": true,
  "[php]": {
    "editor.defaultFormatter": "open-southeners.laravel-pint"
  },
  "phpParameterHint.fontStyle": "normal",
  "phpParameterHint.fontWeight": 600,
  "phpParameterHint.horizontalPadding": 4,
  "phpParameterHint.verticalPadding": 3,
  "extensions.ignoreRecommendations": true,
  "security.workspace.trust.untrustedFiles": "open",
  "intelephense.codeLens.implementations.enable": true,
  "intelephense.codeLens.overrides.enable": true,
  "intelephense.codeLens.parent.enable": true,
  "intelephense.codeLens.references.enable": true,
  "intelephense.codeLens.usages.enable": true,
  "workbench.activityBar.location": "hidden",
  "markdown-preview-enhanced.enablePreviewZenMode": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.cursorBlinking": "smooth"
}
